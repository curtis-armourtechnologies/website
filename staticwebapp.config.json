{
    "routes": [
        {
            "route": "/",
            "serve": "/index.html"
        },
        {
            "route": "/about",
            "serve": "/pages/about.html",
            "statusCode": 200
        },
        {
            "route": "/contact",
            "serve": "/pages/contact.html",
            "statusCode": 200
        },
        {
            "route": "/servicespricing",
            "serve": "/pages/servicespricing.html",
            "statusCode": 200
        },
        {
            "route": "/order-confirmation",
            "serve": "/pages/order-confirmation.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/pages/404.html",
        "exclude": [
            "/images/*.{png,jpg,gif,ico,webm,jpeg}",
            "/css/*",
            "/js/*",
            "/robots.txt",
            "/sitemap.xml"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/pages/404.html",
            "statusCode": 404
        }
    },
    
    "mimeTypes": {
        ".json": "text/json"
    }
}
{
    "routes": [
        {
            "route": "/servicespricing/*",
            "serve": "/pages/servicespricing.html"
        },
        {
            "route": "/about/*",
            "serve": "/pages/about.html"
        },
        {
            "route": "/contact/*",
            "serve": "/pages/contact.html"
        },
        {
            "route": "/order-confirmation/*",
            "serve": "/pages/order-confirmation.html"
        },
        {
            "route": "/*",
            "serve": "/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "/pages/404.html"
    },
    "globalHeaders": {
        "cache-control": "no-cache",
        "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
        "X-Content-Type-Options": "nosniff"
    },
    "mimeTypes": {
        ".html": "text/html",
        ".json": "text/json"
    }
}
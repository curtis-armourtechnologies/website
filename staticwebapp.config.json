{
    "routes": [
        {
            "route": "/servicespricing",
            "serve": "/pages/servicespricing.html",
            "headers": {
                "cache-control": "no-store, no-cache, must-revalidate"
            }
        },
        {
            "route": "/about",
            "serve": "/pages/about.html",
            "headers": {
                "cache-control": "no-store, no-cache, must-revalidate"
            }
        },
        {
            "route": "/contact",
            "serve": "/pages/contact.html",
            "headers": {
                "cache-control": "no-store, no-cache, must-revalidate"
            }
        },
        {
            "route": "/order-confirmation",
            "serve": "/pages/order-confirmation.html",
            "headers": {
                "cache-control": "no-store, no-cache, must-revalidate"
            }
        },
        {
            "route": "/",
            "serve": "/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "/pages/404.html",
        "exclude": ["/images/*", "/css/*", "/js/*"]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/pages/404.html",
            "statusCode": 404
        }
    }
}
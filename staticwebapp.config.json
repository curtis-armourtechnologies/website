{
    "routes": [
        {
            "route": "/servicespricing",
            "serve": "/pages/servicespricing.html",
            "statusCode": 200,
            "headers": {
                "cache-control": "no-store, must-revalidate",
                "Content-Type": "text/html; charset=UTF-8"
            }
        },
        {
            "route": "/about",
            "serve": "/pages/about.html",
            "statusCode": 200,
            "headers": {
                "cache-control": "no-store, must-revalidate",
                "Content-Type": "text/html; charset=UTF-8"
            }
        },
        {
            "route": "/contact",
            "serve": "/pages/contact.html",
            "statusCode": 200,
            "headers": {
                "cache-control": "no-store, must-revalidate",
                "Content-Type": "text/html; charset=UTF-8"
            }
        },
        {
            "route": "/order-confirmation",
            "serve": "/pages/order-confirmation.html",
            "statusCode": 200,
            "headers": {
                "cache-control": "no-store, must-revalidate",
                "Content-Type": "text/html; charset=UTF-8"
            }
        },
        {
            "route": "/",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/pages/404.html"
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/pages/404.html",
            "statusCode": 404
        }
    }
}
{
    "routes": [
        {
            "route": "/",
            "serve": "/index.html"
        },
        {
            "route": "/about",
            "serve": "/pages/about.html",
            "statusCode": 200
        },
        {
            "route": "/contact",
            "serve": "/pages/contact.html",
            "statusCode": 200
        },
        {
            "route": "/servicespricing",
            "serve": "/pages/servicespricing.html",
            "statusCode": 200
        },
        {
            "route": "/order-confirmation",
            "serve": "/pages/order-confirmation.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/pages/404.html",
        "exclude": [
            "/images/*.{png,jpg,gif,ico,webm,jpeg}",
            "/css/*",
            "/js/*",
            "/robots.txt",
            "/sitemap.xml"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/pages/404.html",
            "statusCode": 404
        }
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self' * data: blob: 'unsafe-inline' 'unsafe-eval'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://*.hubspot.com https://*.hsforms.com https://*.hs-scripts.com https://cdnjs.cloudflare.com https://*.fontawesome.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://cdnjs.cloudflare.com https://*.fontawesome.com; font-src 'self' https://fonts.gstatic.com https://cdnjs.cloudflare.com https://*.fontawesome.com data:; frame-src *; connect-src *",
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "SAMEORIGIN",
        "X-XSS-Protection": "1; mode=block",
        "Referrer-Policy": "strict-origin-when-cross-origin"
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}